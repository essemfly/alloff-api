scalar Upload

type ProductGroup {
  id: ID!
  title: String!
  shortTitle: String!
  instruction: [String!]!
  imgUrl: String!
  products: [Product]!
  startTime: Date!
  finishTime: Date!
  numAlarms: Int!
  setAlarm: Boolean!
}

type Faults {
  image: String
  description: String!
}

type Instruction {
  thumbnail: String!
  title: String!
  description: [String!]
  images: [String!]
}

extend type Query {
  productGroup(id: String!): ProductGroup!
  productGroups: [ProductGroup]!
}
